.header-container {
  background-color: #fff;
  margin-top: 5rpx;
  width: 100%;
  display: flex;
  justify-content: center;
  flex-direction: column;
}
.header-container .header {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  font-weight: 900;
}
.header-container .header span {
  margin-left: 50rpx;
}
.header-container .content {
  padding: 10rpx 20rpx 20rpx 20rpx;
  left: auto;
  background-color: #e0dbdb;
}
